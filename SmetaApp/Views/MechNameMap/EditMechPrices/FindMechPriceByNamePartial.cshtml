@model IEnumerable<SmetaApp.Models.MechNameMap>

<div class="result-space">
    @foreach (var j in Model)
    {
        <div onclick="$('#findname').val($(this).text());" class="find-result">@j.MechName</div>
    }
</div>

<script>
    if ("@Model.Any()"== "False")
        $(".result-space").hide();
</script>